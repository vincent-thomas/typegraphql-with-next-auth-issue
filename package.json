    "@types/mocha": "^9.1.1",
    "@types/node": "18.6.4",
    "@types/prettier": "^2",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@yarnpkg/pnpify": "^4.0.0-rc.14",
    "concurrently": "^7.3.0",
    "cypress": "^10.4.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "local-cypress": "^1.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "4.1.1",
