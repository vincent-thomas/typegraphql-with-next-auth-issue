{
  "name": "2tdpn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "develop": "dotenv -c local run-p develop:run develop:generate:watch",
    "develop:deps": "docker compose up -d",
    "develop:run": "next dev",
    "develop:generate": "start-server-and-test develop:run http://localhost:3000 'graphql-codegen' ; pnpify prisma generate",
    "develop:generate:watch": "graphql-codegen --watch",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "e2e:open": "cypress open",
    "e2e:install": "cypress install",
    "e2e:run": "cypress run"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "4.1.1",
    "@tanstack/react-query": "^4.0.10",
    "apollo-server-core": "^3.10.0",
    "apollo-server-micro": "^3.10.0",
    "axios": "^0.27.2",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6",
    "micro": "^9.4.1",
    "next": "12.2.4",
    "next-auth": "^4.10.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "v8": "^0.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.3",
    "@graphql-codegen/typescript": "^2.7.2",
